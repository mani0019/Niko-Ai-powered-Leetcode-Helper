{
    "manifest_version": 3,
    "name": "Neko",
    "version": "1.0",

    "permissions":[
        "tabs",
        "scripting",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "host_permissions": [
        "https://leetcode.com/problems/*", 
        "<all_urls>"
    ],
   
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html"
}